// 简单题，考察对于哈希表的使用，为什么不用pair或者数组？可能记录的时候都是一样，但是想要查看的时候
// 时间复杂度就上来了
class Solution {
public:
    char firstUniqChar(string s) {
        unordered_map<char,int> hash;
        for(auto& str:s){
            if(hash.count(str) == 0){
                hash.emplace(str,1);
            }else{
                ++hash[str];
            }
        }
        for(auto& str:s){
            if(hash[str]==1){
                return str;
            }
        }
        return ' ';
    }
};
