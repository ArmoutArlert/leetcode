// 使用快排思想
class Solution {
public:
    vector<int> exchange(vector<int>& nums) {
        if(nums.empty()){
            return nums;
        }
        int odd = -1;
        int p = 0;

        while(p <= nums.size()-1){
            if(nums[p]%2){
                swap(nums[++odd],nums[p++]);
            }else{
                p++;
            }
        }
        return nums;
    }
};
