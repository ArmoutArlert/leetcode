// 参照答案写的
class Solution {
public:
    vector<string> vec;
    void bfs(string& s,int index){
        if(index == s.size()-1){
            vec.push_back(s);
            return;
        }
        unordered_set<int> hash;
        for(int i=index;i<s.size();i++){
            if(hash.count(s[i])==1){continue;}
            hash.insert(s[i]);
            swap(s[i],s[index]);
            bfs(s,index+1);
            swap(s[i],s[index]);
        }
    }
    vector<string> permutation(string s) {
        bfs(s,0);
        return vec;
    }
};
